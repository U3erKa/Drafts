{
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS",
      "esModuleInterop": true
    }
  },
  "compilerOptions": {
    "allowJs": true,
    "target": "ESNext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist",
    "checkJs": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
  },
  // "include": [
  //   "src"
  // ]
}
