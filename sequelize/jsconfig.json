{
  "compilerOptions": {
    "checkJs": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
